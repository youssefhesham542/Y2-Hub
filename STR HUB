-- قسم سكنات بنات
local selectedOption = nil
TabSkins:AddSection({ Name = "سكنات بنات" })
local Dropdown = TabSkins:AddDropdown({
    Name = "حـددِ سـكن",
    Options = {"سـكن بنـت {1}","سـكن بنـت {2}","سـكن بنـت {3}","سـكن بنـت {4}","سـكن بـنت {5}","سـكن بنـت {6}"},
    Callback = function(option) selectedOption = option end
})
TabSkins:AddButton({
    Name = "طـبقِ الأسـكن",
    Callback = function()
        if not selectedOption then return end
        local Replicated = game:GetService("ReplicatedStorage").Remotes
        local function applyWearItems(items)
            for _, item in ipairs(items) do
                Replicated.Wear:InvokeServer(item)
                wait(1)
            end
        end

        local args = {
            [1] = {96491916349570, 76683091425509, 75159926897589, 1, 1, 3210773801}
        }
        Replicated.ChangeCharacterBody:InvokeServer(unpack(args))
        wait(2)

        if selectedOption == "سـكن بنـت {1}" then
            applyWearItems({15701713751,18509805623,18744734552,15222846056,101459562936324,17529187838,130491506065838,17444483167,16709737106,15395115525,14762227337,6174066797,7581474755})
        elseif selectedOption == "سـكن بنـت {2}" then
            applyWearItems({13307477554,12563952028,15795056785,11156841853,17744851762,16139700318,116091391891300,13133257230,13620518518,18510929286,7675094321})
        elseif selectedOption == "سـكن بنـت {3}" then
            applyWearItems({173569970599873,71333952559271,129864383052397,17744851762,122223238457929,88966032649180,127228549233812,9151422607,18923672769,137160650691565,6238758375,14402624573,13900309877,5981620229,13329302128})
        elseif selectedOption == "سـكن بنـت {4}" then
            applyWearItems({118075313675296,13575374227,17744851762,81209049520605,7667832719,17491092069,17450914193,15827255744,13900309877,15264863474,18236339514,13933994153,8095555899,6433477241})
        elseif selectedOption == "سـكن بـنت {5}" then
            applyWearItems({16163146382,2550890393,15590550272,90156423533584,13900309877,16160920138,6201860992,93406436694938,14619788675,98341593609358,17279703215,17442266260,17561748169,17279701810,114704060956590})
