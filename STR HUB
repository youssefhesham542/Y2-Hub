--========================--
--       تاب الاسكنات      --
--========================--

local Players = game:GetService("Players")
local Remotes = game:GetService("ReplicatedStorage"):WaitForChild("Remotes")
local Target

local TabSkins = Window:MakeTab({
    Title = "الاسكنات",
    Icon  = "rbxassetid://97937665580776"
})

-- قسم نسخ السكنات
local Section = TabSkins:AddSection({ Name = "نسخ سكنات" })

local function GetPlayerNames()
    local names = {}
    for _, p in ipairs(Players:GetPlayers()) do
        table.insert(names, p.Name)
    end
    return names
end

local PlayerDropdown = TabSkins:AddDropdown({
    Name = "اختر لاعب لنسخ سكنه",
    Options = GetPlayerNames(),
    Callback = function(value) Target = value end
})
Players.PlayerAdded:Connect(function() PlayerDropdown:Refresh(GetPlayerNames(), true) end)
Players.PlayerRemoving:Connect(function() PlayerDropdown:Refresh(GetPlayerNames(), true) end)

TabSkins:AddButton({
    Name = "نسخ السكن",
    Callback = function()
        if not Target then return end
        local LP = Players.LocalPlayer
        local LChar = LP.Character
        local TPlayer = Players:FindFirstChild(Target)
        if TPlayer and TPlayer.Character and LChar then
            local LHum = LChar:FindFirstChildOfClass("Humanoid")
            local THum = TPlayer.Character:FindFirstChildOfClass("Humanoid")
            if LHum and THum then
                local PDesc = THum:GetAppliedDescription()
                Remotes.ChangeCharacterBody:InvokeServer({
                    [1]=PDesc.Torso,[2]=PDesc.RightArm,[3]=PDesc.LeftArm,
                    [4]=PDesc.RightLeg,[5]=PDesc.LeftLeg,[6]=PDesc.Head
                })
                task.wait(0.5)
                if tonumber(PDesc.Shirt) then Remotes.Wear:InvokeServer(tonumber(PDesc.Shirt)) end
                if tonumber(PDesc.Pants) then Remotes.Wear:InvokeServer(tonumber(PDesc.Pants)) end
                if tonumber(PDesc.Face) then Remotes.Wear:InvokeServer(tonumber(PDesc.Face)) end
                for _, v in ipairs(PDesc:GetAccessories(true)) do
                    if v.AssetId then Remotes.Wear:InvokeServer(tonumber(v.AssetId)) end
                end
            end
        end
    end
})

-- قسم الورود
local flowersOptions = {
    ["ورده ورديـه"] = 12456323159,
    ["ورده حـمره"] = 12483819104,
    ["ورده رصـاصيه"] = 12659486247,
    ["ورده سـوده"] = 12659123824
}
local selectedFlowerOption
TabSkins:AddSection({ Name = "ورود" })
TabSkins:AddDropdown({
    Name = "اختر ورده",
    Options = {"ورده ورديـه","ورده حـمره","ورده رصـاصيه","ورده سـوده"},
    Callback = function(option) selectedFlowerOption = option end
})
TabSkins:AddButton({
    Name = "تطبيق الورده",
    Callback = function()
        if selectedFlowerOption then
            Remotes.Wear:InvokeServer(flowersOptions[selectedFlowerOption])
        end
    end
})

